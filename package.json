{
  "name": "ipdd-poster-template",
  "version": "1.0.0",
  "description": "A template to bootstrap an IPDD Poster",
  "homepage": "https://ipdd.hfg-karlsruhe.de",
  "scripts": {
    "test": "parcel serve test/index.html --open",
    "serve": "parcel serve src/index.html --open",
    "bundle": "node ./bundler"
  },
  "browser": "dist/index.html",
  "targets": {
    "browser": {
      "engines": {
        "browsers": "> 0.25%"
      },
      "publicUrl": ".",
      "scopeHoist": true,
      "sourceMap": false
    }
  },
  "keywords": [
    "IPDD",
    "Template"
  ],
  "author": "Grischa Erbe <grischa.erbe@googlemail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/legrisch/ipdd-poster-template.git"
  },
  "license": "ISC",
  "devDependencies": {
    "adm-zip": "^0.5.1",
    "clipboard": "^2.0.6",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "image-size": "^0.9.3",
    "marked": "^1.2.5",
    "parcel": "^2.0.0-beta.1",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0"
  },
  "dependencies": {
    "normalize-scss": "^7.0.1"
  }
}